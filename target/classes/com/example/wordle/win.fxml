<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="300.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.wordle.winController">
   <children>
      <Label alignment="CENTER" contentDisplay="CENTER" layoutY="32.0" prefHeight="66.0" prefWidth="300.0" text="You Win!">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Button fx:id="newGame" alignment="CENTER" contentDisplay="CENTER" layoutX="85.0" layoutY="129.0" mnemonicParsing="false" prefHeight="43.0" prefWidth="131.0" text="New Game" onAction="#onButtonClicked" />
      <Button fx:id="exitbtn" alignment="CENTER" contentDisplay="CENTER" layoutX="84.0" layoutY="187.0" mnemonicParsing="false" prefHeight="43.0" prefWidth="131.0" text="Exit" onAction="#onButtonClicked"/>
   </children>
</AnchorPane>
